# Database Configuration
DATABASE_URL=jdbc:postgresql://localhost:5432/baro_db
DATABASE_USERNAME=your_database_username
DATABASE_PASSWORD=your_database_password

# JWT Configuration (Secret key should be 256+ bits)
JWT_SECRET_KEY=your-very-long-and-secure-secret-key-at-least-256-bits
JWT_ACCESS_VALIDITY_SECONDS=3600
JWT_REFRESH_VALIDITY_SECONDS=604800

# Email Configuration (IMAP)
MAIL_IMAP_HOST=smtp.gmail.com
MAIL_IMAP_USERNAME=your-email@gmail.com
MAIL_IMAP_PASSWORD=your-app-password

# Google Maps API
GOOGLE_MAPS_API_KEY=your-google-maps-api-key

# Google Imagen Image Generation API
# Imagen 3.0 모델 사용 (공식 이미지 생성 API)
# 공식 무료 티어 할당량: RPM=10 (분당 10회), RPD=100 (일일 100회)
# API 문서: https://ai.google.dev/gemini-api/docs/imagen
GOOGLE_GEMINI_API_URL=https://generativelanguage.googleapis.com/v1beta/models/imagen-3.0-generate-002:predict
GOOGLE_GEMINI_API_KEY=your-gemini-api-key

# Google Gemini API Quota & Retry Configuration
# RPM (분당 요청) 제한을 고려한 재시도 설정
GOOGLE_GEMINI_RETRY_MAX_ATTEMPTS=3
GOOGLE_GEMINI_RETRY_DELAY=10

# Rate Limiting (공식 무료 티어 기준)
GOOGLE_GEMINI_QUOTA_ENABLED=true
GOOGLE_GEMINI_QUOTA_RPM=10
GOOGLE_GEMINI_QUOTA_RPD=100
GOOGLE_GEMINI_DAILY_LIMIT=100

# Firebase Configuration
FIREBASE_PROJECT_ID=baroyofi
FIREBASE_CREDENTIALS_PATH=firebase-adminsdk.json